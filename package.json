{
  "name": "cumulocity-smart-apps-webpack",
  "version": "1.0.2",
  "description": "A boilerplate for creating Cumulocity smart app using webpack-dev-server 2.x",
  "keywords": [
    "cumulocity",
    "m2m",
    "examples",
    "demo",
    "boilerplate",
    "cumulocity smart app",
    "cumulocity webpack",
    "cumulocity toolkit"
  ],
  "homepage": "https://github.com/dwiyatci/cumulocity-smart-apps-webpack",
  "bugs": "https://github.com/dwiyatci/cumulocity-smart-apps-webpack/issues",
  "license": "MIT",
  "author": "Glenn Dwiyatcita <glenn.dwiyatcita@cumulocity.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/cumulocity-smart-apps-webpack.git"
  },
  "scripts": {
    "install:clean": "rimraf node_modules bower_components && npm i && bower i",
    "update": "updtr -e angular,bootstrap-sass,d3,jquery,lodash,moment,q",
    "start": "webpack-dev-server --hot",
    "build": "npm run lint && npm run clean && webpack --env.prod",
    "lint": "eslint --cache src",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "angular": "1.2.29",
    "bootstrap-sass": "3.3.5",
    "d3": "3.5.5",
    "jquery": "2.1.1",
    "lodash": "2.4.1",
    "moment": "2.11.1"
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-es2015-webpack": "6.4.3",
    "babel-preset-stage-2": "6.13.0",
    "bootstrap-loader": "2.0.0-beta.3",
    "css-loader": "0.23.1",
    "eslint": "3.3.1",
    "eslint-config-airbnb": "10.0.1",
    "eslint-plugin-import": "1.13.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.1.1",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "0.9.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "node-sass": "3.8.0",
    "resolve-url-loader": "1.6.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "updtr": "0.2.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.20",
    "webpack-dev-server": "2.1.0-beta.0"
  }
}
